<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.javashooter.connection.database.PlayersEntity" table="players" schema="main">
        <id name="name">
            <column name="name" sql-type="text(255)" not-null="true"/>
        </id>
        <property name="wins">
            <column name="wins" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>